<!-- 海 阔 天 空 -->
<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <LinearGradientBrush x:Key="Background_SidePanel" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Color="#5292c0" Offset="0"/>
        <GradientStop Color="#3f689e" Offset="1"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="Background_MainPanel" StartPoint="0,0" EndPoint="1,1">
        <GradientStop Color="#3f689e" Offset="0"/>
        <GradientStop Color="#355389" Offset="1"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="Background_Tunnel" StartPoint="0,0" EndPoint="0.5,1">
        <GradientStop Color="#5292c0" Offset="0"/>
        <GradientStop Color="#3f689e" Offset="1"/>
    </LinearGradientBrush>

    <Color x:Key="TabGradient_Start">#bedeed</Color>
    <Color x:Key="TabGradient_Middle">#94d3f2</Color>
    <Color x:Key="TabGradient_End">#5e90c1</Color>
    <Color x:Key="TabGradient_EndMiddle">#ee5e90c1</Color>
    <Color x:Key="TabGradient_EndTransparent">#005e90c1</Color>
</ResourceDictionary>