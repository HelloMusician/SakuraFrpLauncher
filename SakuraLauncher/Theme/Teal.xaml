<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <LinearGradientBrush x:Key="Background_SidePanel" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Color="#1F5966" Offset="0"/>
        <GradientStop Color="#0F2C33" Offset="1"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="Background_MainPanel" StartPoint="0,0" EndPoint="1,1">
        <GradientStop Color="#0F2C33" Offset="0"/>
        <GradientStop Color="#00151A" Offset="1"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="Background_Tunnel" StartPoint="0,0" EndPoint="0.5,1">
        <GradientStop Color="#0092B3" Offset="0"/>
        <GradientStop Color="#005366" Offset="1"/>
    </LinearGradientBrush>

    <Color x:Key="TabGradient_Start">#34e89e</Color>
    <Color x:Key="TabGradient_Middle">#259f79</Color>
    <Color x:Key="TabGradient_End">#0f6043</Color>
    <Color x:Key="TabGradient_EndMiddle">#ee0f6043</Color>
    <Color x:Key="TabGradient_EndTransparent">#000f6043</Color>
</ResourceDictionary>