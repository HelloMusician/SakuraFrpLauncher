<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <LinearGradientBrush x:Key="Background_SidePanel" StartPoint="1,0" EndPoint="0,1">
        <GradientStop Offset="0" Color="#3b3b3b" />
        <GradientStop Offset="1" Color="#111111" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="Background_MainPanel" StartPoint="0,0" EndPoint="1,1">
        <GradientStop Offset="0" Color="#3b3b3b" />
        <GradientStop Offset="1" Color="#111111" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="Background_Tunnel" StartPoint="0,0" EndPoint="0.5,1">
        <GradientStop Offset="0" Color="#e5b266" />
        <GradientStop Offset="1" Color="#be853d" />
    </LinearGradientBrush>

    <Color x:Key="TabGradient_Start">#fee26d</Color>
    <Color x:Key="TabGradient_Middle">#e6a72c</Color>
    <Color x:Key="TabGradient_End">#66fee26d</Color>
    <Color x:Key="TabGradient_EndMiddle">#eefee26d</Color>
    <Color x:Key="TabGradient_EndTransparent">#00fee26d</Color>
</ResourceDictionary>